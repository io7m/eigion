<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?><?import javafx.scene.layout.StackPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
            prefHeight="600.0"
            prefWidth="800.0"
            xmlns="http://javafx.com/javafx/18"
            fx:controller="com.io7m.eigion.gui.internal.EIGMainController">
  <StackPane AnchorPane.bottomAnchor="0.0"
             AnchorPane.leftAnchor="0.0"
             AnchorPane.rightAnchor="0.0"
             AnchorPane.topAnchor="0.0">
    <children>
      <fx:include source="dashboard.fxml" fx:id="dashboardLayout"/>
      <fx:include source="login.fxml" fx:id="loginLayout"/>
      <fx:include source="logo.fxml" fx:id="logoLayout"/>
    </children>
  </StackPane>
</AnchorPane>
